{
  "parser": "babel-eslint",
  "plugins": ["node"],
  "extends": ["eslint:recommended", "plugin:node/recommended"],
  "rules": {
    "no-console": "warn",
    "comma-dangle": [2, "never"],
    "semi": 0,
    "max-len": 0,
    "no-multiple-empty-lines": 0,
    "object-shorthand": 0,
    "padded-blocks": 0
  }
}
