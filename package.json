{
  "name": "timeline-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-plugin-node": "^6.0.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.2.0",
    "mockgoose": "^7.3.5",
    "nodemon": "^1.17.4",
    "prettier": "^1.13.5",
    "supertest": "^3.0.0"
  },
  "scripts": {
    "start": "nodemon --exec babel-node -- index",
    "test": "jest --runInBand --forceExit --coverage",
    "test-coverage": "yarn test && codecov",
    "lint": "eslint ./**/*.js",
    "pretty": "prettier --write \"src/**/*.js\"",
    "precommit": "lint-staged && yarn test"
  },
  "lint-staged": {
      "*.js": [
          "yarn pretty",
          "git add"
      ]
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "mongoose": "^5.1.4",
    "morgan": "^1.9.0",
    "ramda": "^0.25.0",
    "uuid": "^3.2.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 91,
        "lines": 92,
        "statements": 92
      }
    }
  },
  "engines": {
    "node": ">=9.0.0"
  }
}
